//
// Global Footer
//

.gf
	&-menu
		background: lighten( $black, 10% )
		height: 150px
		// height: 100%
	
	&-nav
		v-align()
		list-style-type: none
		&__link
			text-decoration: none
			color: hsl(210, 93%, 62%)
			font-size: 18px
			&:hover
				color: darken(hsl(210, 100%, 44%), 10%)
		&__item
			display: inline